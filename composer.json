{
    "name": "paulbunyannet/wordpress-artisan",
    "description": "Artisan commands to interact with Wordpress",
    "keywords": ["laravel", "wordpress"],
    "homepage": "https://github.com/paulbunyannet/wordpress-artisan",
    "license": "MIT",
    "authors": [
        {
            "name": "Nate Nolting",
            "email": "naten@paulbunyan.net",
            "homepage": "http://www.paulbunyan.net"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "illuminate/console": "^12.0",
        "illuminate/filesystem": "^12.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^11.0",
        "orchestra/testbench": "^10.0",
        "mockery/mockery": "~1",
        "fakerphp/faker": "^1.9.2"
    },
    "autoload": {
        "psr-4": {
            "Pbc\\WordpressArtisan\\": "src"
        },
        "classmap": [
            "tests"
        ]
    },
  "scripts": {
    "test": [
      ""
    ]
  },
    "suggest": {
        "jgrossi/corcel": "Needed for using wp:clear-cache command. See https://github.com/corcel/corcel for instructions."
    }
}
